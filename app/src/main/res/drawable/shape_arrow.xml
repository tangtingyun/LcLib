<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate>
            <shape android:shape="rectangle">
                <size
                    android:width="30dp"
                    android:height="30dp" />
                <stroke
                    android:width="10dp"
                    android:color="@android:color/transparent" />
                <solid android:color="#000000" />
            </shape>
        </rotate>
    </item>
    <item>
        <rotate android:fromDegrees="45"
            android:pivotY="20">
            <shape android:shape="rectangle">
                <size
                    android:width="30dp"
                    android:height="30dp" />
                <stroke
                    android:width="10dp"
                    android:color="@android:color/transparent" />
                <solid android:color="#000000" />
            </shape>
        </rotate>
    </item>

